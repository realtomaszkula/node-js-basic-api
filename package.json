    "babel-preset-env": "^1.5.1",
    "faker": "^4.1.0",
    "jasmine": "^2.6.0",
    "jasmine-console-reporter": "^1.2.7",
    "json-schema-faker": "^0.5.0-rc3",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "chalk": "^1.1.3",
    "dotenv": "^4.0.0",
